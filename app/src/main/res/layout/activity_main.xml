<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="thedoctors05.tentfinder.MainActivity"
    android:weightSum="1"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp"
        android:id="@+id/linearLayout"></LinearLayout>

    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="11dp">

    </ListView>

    <Button
        android:id="@+id/bAddTent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/add_new_tent"
        android:onClick="addTentActivity"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/bNavigate" />

    <Button
        android:id="@+id/bNavigate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:text="Navigate"
        android:layout_alignParentEnd="true"
        android:onClick="navigationActivity"/>

</RelativeLayout>
